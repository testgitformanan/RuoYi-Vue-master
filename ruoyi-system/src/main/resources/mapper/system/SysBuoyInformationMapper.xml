<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SysBuoyInformationMapper">
    
    <resultMap type="SysBuoyInformation" id="SysBuoyInformationResult">
        <result property="id"    column="id"    />
        <result property="code"    column="code"    />
        <result property="content"    column="content"    />
        <result property="informationFileName"    column="information_file_name"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectSysBuoyInformationVo">
        select id, code, content, information_file_name, create_by, create_time, update_by, update_time from sys_buoy_information
    </sql>

    <select id="selectSysBuoyInformationList" parameterType="SysBuoyInformation" resultMap="SysBuoyInformationResult">
        <include refid="selectSysBuoyInformationVo"/>
        <where>  
            <if test="code != null  and code != ''"> and code = #{code}</if>
            <if test="content != null  and content != ''"> and content = #{content}</if>
            <if test="informationFileName != null  and informationFileName != ''"> and information_file_name like concat('%', #{informationFileName}, '%')</if>
        </where>
    </select>
    
    <select id="selectSysBuoyInformationById" parameterType="Long" resultMap="SysBuoyInformationResult">
        <include refid="selectSysBuoyInformationVo"/>
        where id = #{id}
    </select>

    <insert id="insertSysBuoyInformation" parameterType="SysBuoyInformation" useGeneratedKeys="true" keyProperty="id">
        insert into sys_buoy_information
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="code != null">code,</if>
            <if test="content != null">content,</if>
            <if test="informationFileName != null">information_file_name,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="code != null">#{code},</if>
            <if test="content != null">#{content},</if>
            <if test="informationFileName != null">#{informationFileName},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateSysBuoyInformation" parameterType="SysBuoyInformation">
        update sys_buoy_information
        <trim prefix="SET" suffixOverrides=",">
            <if test="code != null">code = #{code},</if>
            <if test="content != null">content = #{content},</if>
            <if test="informationFileName != null">information_file_name = #{informationFileName},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteSysBuoyInformationById" parameterType="Long">
        delete from sys_buoy_information where id = #{id}
    </delete>

    <delete id="deleteSysBuoyInformationByIds" parameterType="String">
        delete from sys_buoy_information where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>