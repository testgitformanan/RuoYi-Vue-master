    <select id="selectWxgzGzztxsList" parameterType="WxgzGzztxs" resultMap="WxgzGzztxsResult">
        <include refid="selectWxgzGzztxsVo"/>
        <where>
            <if test="type != null ">and type = #{type}</if>
            <if test="status != null  and status != ''">and status = #{status}</if>
            <if test="date != null  and date != ''">and date = #{date}</if>
            <if test="lng != null  and lng != ''">and lng = #{lng}</if>
            <if test="lat != null  and lat != ''">and lat = #{lat}</if>
            <if test="content != null  and content != ''">and content = #{content}</if>
            <if test="params.beginTime != null and params.beginTime != ''"><!-- 开始时间检索 -->
                AND date &gt;= #{params.beginTime}
            </if>
            <if test="params.endTime != null and params.endTime != ''"><!-- 结束时间检索 -->
                AND date &lt;= #{params.endTime}
            </if>
        </where>
    </select>

    <select id="selectWxgzGzztxsById" parameterType="Long" resultMap="WxgzGzztxsResult">
        <include refid="selectWxgzGzztxsVo"/>
        where id = #{id}
    </select>

    <insert id="insertWxgzGzztxs" parameterType="WxgzGzztxs" useGeneratedKeys="true" keyProperty="id">
        insert into wxgz_gzztxs
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="type != null">type,</if>
            <if test="status != null">status,</if>
            <if test="date != null">date,</if>
            <if test="lng != null">lng,</if>
            <if test="lat != null">lat,</if>
            <if test="content != null">content,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="type != null">#{type},</if>
            <if test="status != null">#{status},</if>
            <if test="date != null">#{date},</if>
            <if test="lng != null">#{lng},</if>
            <if test="lat != null">#{lat},</if>
            <if test="content != null">#{content},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
        </trim>
    </insert>

    <update id="updateWxgzGzztxs" parameterType="WxgzGzztxs">
        update wxgz_gzztxs
        <trim prefix="SET" suffixOverrides=",">
            <if test="type != null">type = #{type},</if>
            <if test="status != null">status = #{status},</if>
            <if test="date != null">date = #{date},</if>
            <if test="lng != null">lng = #{lng},</if>
            <if test="lat != null">lat = #{lat},</if>
            <if test="content != null">content = #{content},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteWxgzGzztxsById" parameterType="Long">
        delete
        from wxgz_gzztxs
        where id = #{id}
    </delete>

    <delete id="deleteWxgzGzztxsByIds" parameterType="String">
        delete from wxgz_gzztxs where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>